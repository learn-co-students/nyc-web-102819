<h1>Create ya Cow ğŸ‘ </h1>


<%= form_for(@cow) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :number_of_spots, "Spots" %>
  <%= f.number_field :number_of_spots %>
  <br>
  <%= f.label :age %>
  <%= f.number_field :age %>
  <br>
  <%= f.collection_select :alien_id, @aliens, :id, :name, prompt: true %>
  <br>
  <%= f.submit %>
<% end %>
